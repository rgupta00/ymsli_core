package com.day2.session2.callbyref;

class Data {
	private int val;

	public int getVal() {
		return val;
	}

	public void setVal(int val) {
		this.val = val;
	}

	public Data(int val) {
		this.val = val;
	}

}

public class DemoCallByRef {

	public static void main(String[] args) {

		Data d1, d2;
		d1 = new Data(22);
		d2 = new Data(33);

		// 22 33
		System.out.println("before swapping: d1: " + d1.getVal() + " d2: " + d2.getVal());

		swap(d1, d2);

		// 22 33
		System.out.println("after swapping: d1: " + d1.getVal() + " d2: " + d2.getVal());
	}
	//when we pass ref of object in a method call, java will pass copy of the reference
	static void swap(Data i, Data j) {
		Data temp;
		temp = i;
		i = j;
		j = temp;
	}

}
